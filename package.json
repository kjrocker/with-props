{
  "name": "with-props",
  "version": "1.0.0",
  "description": "HOC For Rearranging React Props",
  "main": "index.ts",
  "types": "index.d.ts",
  "scripts": {
    "dist": "tsc --declaration",
    "dist:watch": "tsc --declaration --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/jest": "^23.1.5",
    "@types/sinon": "^5.0.1",
    "@types/react-dom": "^16.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "sinon": "^6.1.3",
    "jest": "^23.3.0",
    "jest-cli": "^23.3.0",
    "typescript": "^2.9.2",
    "ts-node": "^7.0.0",
    "ts-jest": "^23.0.0"
  },
  "keywords": [
    "react",
    "props",
    "hoc"
  ],
  "jest": {
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text-summary"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.(ts|tsx)",
      "!<rootDir>/src/**/*.test.(ts|tsx)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    }
  },
  "author": "Kevin Rocker",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
